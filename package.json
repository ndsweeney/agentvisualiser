{
  "name": "agentfactory-monorepo",
  "version": "1.0.0",
  "description": "AgentFactory - Production-ready multi-agent system platform",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "dev:api": "npm run dev --workspace=@agentfactory/api",
    "dev:web": "npm run dev --workspace=@agentfactory/web",
    "start:api": "npm run start --workspace=@agentfactory/api",
    "start:web": "npm run start --workspace=@agentfactory/web",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "agents",
    "multi-agent-systems",
    "workflow",
    "automation",
    "typescript",
    "nestjs",
    "nextjs"
  ],
  "author": "AgentFactory Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agentfactory/agentfactory.git"
  }
}